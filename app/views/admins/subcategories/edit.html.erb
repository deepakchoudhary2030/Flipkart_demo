<div class="container">
<h1>Edit Your Details</h1>
<%= form_with(model:@subcategory,url:admins_subcategory_path(@subcategory),method: :patch) do |form| %>
  <div class="form-outline mb-4">
    <%= form.label :name %><br>
    <%= form.text_field :name ,class: "form-control "  %>
  </div>
    <div class="form-outline mb-4">
      <%= form.label :image %>
      <%= form.file_field :cover %>
   </div>
  <div>
    <%=  form.submit :save, class: 'btn btn-success' %>
  </div>
<% end %>

</div>